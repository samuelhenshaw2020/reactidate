{
  "name": "reactidate",
  "private": false,
  "version": "1.0.11",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint . --ext .ts",
    "coverage": "vitest run --coverge",
    "prepare": "husky install",
    "security": "npm audit",
    "version": "npm version patch",
    "ci": "act"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.11.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "@vitejs/plugin-react": "^2.1.0",
    "eslint": "^8.25.0",
    "eslint-plugin-react": "^7.31.10",
    "happy-dom": "^7.5.12",
    "husky": "^8.0.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^1.6.6",
    "vitest": "^0.24.3"
  },
  "files": [
    "dist",
    "dist/**/*.ts"
  ],
  "main": "./dist/reactidate.umd.js",
  "module": "./dist/reactidate.es.js",
  "exports": {
    "import": "./dist/reactidate.es.js",
    "require": "./dist/reactidate.umd.js"
  },
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/samuelhenshaw2020/reactidate.git"
  },
  "bugs": {
    "url": "https://github.com/samuelhenshaw2020/reactidate/issues"
  },
  "author": {
    "name": "Samuel Henshaw"
  },
  "keywords": [
    "React",
    "Form Validation",
    "Form",
    "Validator",
    "Simple"
  ],
  "description": "React package for form validation",
  "engines": {
    "node": ">=10"
  },
  "homepage": "https://github.com/samuelhenshaw2020/reactidate#readme",
  "license": "MIT",
  "access": "public",
  "dependencies": {
    "reactidate": "^1.0.9"
  }
}
